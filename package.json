{
  "name": "@ryantamulevicz/convex-better-auth-astro",
  "version": "0.1.0",
  "description": "Astro integration helpers for combining better-auth with Convex",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "clean": "rm -rf dist",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "keywords": [
    "astro",
    "convex",
    "better-auth",
    "auth",
    "cookies"
  ],
  "author": "Ryan Tamulevicz",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RyanTamulevicz/convex-better-auth-astro.git"
  },
  "bugs": {
    "url": "https://github.com/RyanTamulevicz/convex-better-auth-astro/issues"
  },
  "homepage": "https://github.com/RyanTamulevicz/convex-better-auth-astro#readme",
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "@better-fetch/fetch": "^1.0.0",
    "astro": "^4.0.0 || ^5.0.0",
    "better-auth": "^1.0.0",
    "convex": "^1.12.0",
    "@convex-dev/better-auth": "^0.9.6"
  },
  "devDependencies": {
    "@better-fetch/fetch": "^1.1.18",
    "astro": "^5.0.0",
    "better-auth": "^1.3.28",
    "convex": "^1.12.0",
    "@types/node": "^20.12.7",
    "tsdown": "^0.2.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
